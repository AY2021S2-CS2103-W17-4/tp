@startuml
!include style.puml
skinparam arrowThickness 1.1
skinparam arrowColor MODEL_COLOR
skinparam classBackgroundColor MODEL_COLOR

Package Client {
Class Client
}

Package Model <<Rectangle>>{
Interface ReadOnlyClientBook <<Interface>>
Interface ReadOnlyMeetingBook <<Interface>>
Interface Model <<Interface>>
Interface ObservableList <<Interface>>
Class ClientBook
Class MeetingBook
Class Model
Class ModelManager
Class UserPrefs
Class ReadOnlyUserPrefs

Package Tag {
Class Tag
}
}

Class HiddenOutside #FFFFFF
HiddenOutside ..> Model

ClientBook .up.|> ReadOnlyClientBook
MeetingBook .up|> ReadOnlyMeetingBook

ModelManager .up.|> Model
Model .right.> ObservableList
ModelManager o--> "1" ClientBook
ModelManager o--> "1" MeetingBook
ModelManager o-left-> "1" UserPrefs
UserPrefs .up.|> ReadOnlyUserPrefs


ModelManager ----->" ~* filtered list" Client
ModelManager ----->" ~* filtered list" Meeting
@enduml
